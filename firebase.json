{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": [
    {
      "target": "theneighbor-hood",
      "public": "/",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/link/**",
          "dynamicLinks": true
        }
      ]
    },
    {
      "target": "filedump",
      "public": "filedump",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/link/**",
          "dynamicLinks": true
        }
      ]
    }
  ],
  "appAssociation": "AUTO",
  "storage": {
    "rules": "storage.rules"
  }
}